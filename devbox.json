{
	"$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.10.5/.schema/devbox.schema.json",
	"packages": [
		"nodejs@25",
		"yarn"
	],
	"shell": {
		"init_hook": [
			"echo 'Starting front-student!' > /dev/null",
			"npm install"
		],
		"scripts": {
			"dev": [
				"npm run dev"
			],
			"build": [
				"npm run build"
			],
			"preview": [
				"npm run preview"
			]
		}
	}

}
